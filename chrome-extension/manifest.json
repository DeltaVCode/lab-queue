{
  "name": "Lab Queue",
  "version": "1.1",
  "description": "A browser action which give access to the TA/Student help queue.",
  "page_action": {
    "default_title": "Lab Queue",
    "default_icon": "q-icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}